{
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Leetcode rich presence extension for Discord",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/background/background.ts"
  },
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": ["https://leetcode.com/*", "https://*.leetcode.com/*"]
}
